{
  "include": ["src/assets/ts/**/*"],
  "compilerOptions": {
    /* Language and Environment */
    "target": "es2016",

    /* Modules */
    "module": "ES2022",

    /* Emit */
    "outDir": "src/assets/js",

    /* Interop Constraints */
    "forceConsistentCasingInFileNames": true,

    /* Type Checking */
    "strict": true
  }
}
